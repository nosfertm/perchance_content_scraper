                      let warningsHTML = "";
                      for(let warning of e.data.warnings) {
                        if(typeof warning.lineNumber !== "number" || typeof warning.warningId !== "string" || /[^a-z0-9\-]/.test(warning.generatorName)) return;
                        let warningTemplate = warnings[warning.warningId];
