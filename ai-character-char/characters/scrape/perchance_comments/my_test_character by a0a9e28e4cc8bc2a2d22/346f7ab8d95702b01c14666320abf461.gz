                this.store.data.user = {email:details.email, sessionToken:details.sessionToken, loggedIn:true};
                this.store.save();
                // this.update();
                this.checkIfOwner();
                this.trigger("LoginStateChange");
              };
              this.emailChangeCallback = (newEmail) => {
                this.store.data.user.email = newEmail;
              };
